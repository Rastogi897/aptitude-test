{{#content "body"}}

<div class="mx-auto max-w-5xl">
	<div class="space-y-16 px-10">
		{{> questions-list}}
	</div>

<nav class="flex items-center justify-between border-t border-gray-200 bg-white px-4 py-3 sm:px-6 mt-20" aria-label="Pagination">
  <div class="hidden sm:block">
    <p class="text-sm text-gray-700">
      Showing
      <span class="font-medium">{{from}}</span>
      to
      <span class="font-medium">{{to}}</span>
      of
      <span class="font-medium">{{total}}</span>
      results
    </p>
  </div>
  <div class="flex flex-1 justify-between sm:justify-end">
    {{#if prevUrl}}<a href="{{prevUrl}}" class="relative inline-flex items-center rounded-md border border-gray-300 bg-white px-4 py-2 text-sm font-medium text-gray-700 hover:bg-gray-50">Previous</a>{{/if}}
    {{#if nextUrl}}<a href="{{nextUrl}}" class="relative ml-3 inline-flex items-center rounded-md border border-gray-300 bg-white px-4 py-2 text-sm font-medium text-gray-700 hover:bg-gray-50">Next</a>{{/if}}
  </div>
</nav>


</div>

	<script>
		var q = {{{JSONstringify questions}}};
	</script>
{{/content}}